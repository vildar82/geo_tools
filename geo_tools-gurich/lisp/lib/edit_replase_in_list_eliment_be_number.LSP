(defun edit_replase_in_list_eliment_be_number (cell n list_cell / new_list_cell x)
  (setq x 1)
  (repeat (length list_cell)
    (if (= x n)
      (setq new_list_cell (append new_list_cell (list cell)))
      (setq new_list_cell (append new_list_cell (list (car list_cell))))
      )
    (setq x (1+ x))
    (setq list_cell (cdr list_cell))
    )
  new_list_cell)
