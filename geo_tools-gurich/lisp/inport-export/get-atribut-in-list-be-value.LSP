(defun get-atribut-in-list-be-value (input-list atribut-name atribut-value / flag n num item)
  ; возвращает элемент списка в котором содержится список '(atribut-name atribut-value)
  (setq num (length input-list))
  (setq n 0)
  (while (not(or(= n num)(> n num)))
    (setq item (nth n input-list))
    (if (member (list atribut-name atribut-value) item)
      (progn (setq n num) (setq flag T))
      (setq n (1+ n))
      )
    )
  (if flag item nil)
  )